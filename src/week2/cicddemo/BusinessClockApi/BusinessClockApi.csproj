    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>d0d8946b-7d14-4eee-a21d-41f3dab9be67</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
